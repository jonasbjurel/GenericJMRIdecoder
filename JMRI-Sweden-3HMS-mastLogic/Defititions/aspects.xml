  <aspects>
	<aspect>
      <name>Stopp</name>
      <rule>Stopp</rule>
      <indication>Stop</indication>
      <speed>Stop</speed>
      <speed2>Stop</speed2>
      <route>Either</route>
      <dccAspect>1</dccAspect>
    </aspect>

    <aspect>
	  <name>Stopp - vänta - begär tillstånd</name>
	  <rule>Stopp - vänta - begär tillstånd</rule>
	  <indication>Stop</indication>
	  <speed>KörFörsiktigt</speed>
	  <speed2></speed2>
	  <route>Either</route>
	  <dccAspect>1</dccAspect>
     </aspect>

    <aspect>
	  <name>Stopp - manuell växling - vänta - begär tillstånd</name>
	  <rule>Stopp - manuell växling - vänta - begär tillstånd</rule>
	  <indication>Stop</indication>
	  <speed>KörKryp</speed>
	  <speed2>KörKryp</speed2>
	  <route>Either</route>
	  <dccAspect>1</dccAspect>
    </aspect>

	<aspect>
	  <name>Stopp - hinder - vänta - begär tillstånd</name>
	  <rule>Stopp - hinder - vänta - begär tillstånd</rule>
	  <indication>Stop</indication>
	  <speed>KörKryp</speed>
	  <speed2>KörKryp</speed2>
	  <route>Either</route>
	  <dccAspect>1</dccAspect>
    </aspect>
	  
    <aspect>
      <name>Kör 80 - vänta</name>
      <rule>Kör 80 - vänta</rule>
      <indication>Proceed 80 KMH and deaccelerate to 40 KMPH for any next signal indication (or ATC indications)</indication>
      <speed>Kör80</speed>
      <speed2>Kör40</speed2>
      <route>Either</route>
      <dccAspect>2</dccAspect>
    </aspect>

	<aspect>
	  <name>Kör 40 - vänta</name>
	  <rule>Kör 40 - vänta</rule>
	  <indication>Proceed 40 KMPH and use caution prepared for any next signal indication</indication>
	  <speed>Kör40</speed>
	  <speed2>Kör40</speed2>
	  <route>Either</route>
	  <dccAspect>3</dccAspect>
	</aspect>

	  <aspect>
		  <name>Kör 40 - vänta - kör 40</name>
		  <rule>Kör 40 - vänta - kör 40</rule>
		  <indication>Proceed 40 KMPH, next signal shows proceed 40 KMPH</indication>
		  <speed>Kör40</speed>
		  <speed2>Kör40</speed2>
		  <route>Either</route>
		  <dccAspect>3</dccAspect>
	  </aspect>

	  <aspect>
		  <name>Kör 40 - vänta - stopp</name>
		  <rule>Kör 40 - vänta - stopp</rule>
		  <indication>Proceed 40 KMPH, next signal shows stop</indication>
		  <speed>Kör40</speed>
		  <speed2>Stop</speed2>
		  <route>Either</route>
		  <dccAspect>3</dccAspect>
	  </aspect>

	  <aspect>
      <name>Kör 80 - vänta - kör 80</name>
      <rule>Kör 80 - vänta - kör 80</rule>
      <indication>Proceed 80 KMPH prepared to pass next signal not exceeding 80 KMPH (or ATC indications)</indication>
      <speed>Kör80</speed>
      <speed2>Kör80</speed2>
      <route>Either</route>
      <dccAspect>4</dccAspect>
    </aspect>
	  
    <aspect>
      <name>Kör 80 - vänta - kör 40</name>
      <rule>Kör 80 - vänta - kör 40</rule>
      <indication>Proceed prepared to pass next signal not exceeding 40 KMPH (or ATC indications)</indication>
      <speed>Kör80</speed>
      <speed2>Kör40</speed2>
      <route>Either</route>
      <dccAspect>5</dccAspect>
	</aspect>

	  <aspect>
		  <name>Kör 80 - vänta - stopp</name>
		  <rule>Kör 80 - vänta - stopp</rule>
		  <indication>Proceed prepared to pass next signal not exceeding 40 KMPH (or ATC indications)</indication>
		  <speed>Kör80</speed>
		  <speed2>Stop</speed2>
		  <route>Either</route>
		  <dccAspect>5</dccAspect>
	  </aspect>

    <aspect>
	  <name>Vänta - stopp</name>
	  <rule>Vänta - stopp</rule>
	  <indication>Proceed prepared to stop at next signal</indication>
	  <speed>Kör40</speed>
	  <speed2>Stop</speed2>
	  <route>Either</route>
	  <dccAspect>7</dccAspect>
    </aspect>

	  <aspect>
		  <name>Vänta - kör 80</name>
		  <rule>Vänta - kör 80</rule>
		  <indication>Proceed to next signal expect approach at 80 KMPH</indication>
		  <speed>Kör80</speed>
		  <speed2>Kör80</speed2>
		  <route>Either</route>
		  <dccAspect>8</dccAspect>
	  </aspect>

	  <aspect>
		  <name>Vänta - kör 40</name>
		  <rule>Vänta - kör 40</rule>
		  <indication>Proceed to next signal expect approach at 40 KMPH</indication>
		  <speed>Kör40</speed>
		  <speed2>Kör40</speed2>
		  <route>Either</route>
		  <dccAspect>9</dccAspect>
	  </aspect>

	  <!-- Helper dummy masts -->
	  <aspect>
		  <name>Hinder aktivering</name>
		  <rule>Aktiverar hinderindikering för dvärgsignaler</rule>
		  <indication>Activates obstruction aspect for dwarfs</indication>
		  <speed>Stop</speed>
		  <speed2>Stop</speed2>
		  <route>Either</route>
		  <dccAspect>9</dccAspect>
	  </aspect>

	  <aspect>
		  <name>Manuell växling aktivering</name>
		  <rule>Manuell växling aktivering</rule>
		  <indication>Activates manual switching aspect for dwarfs</indication>
		  <speed>Stop</speed>
		  <speed2>Stop</speed2>
		  <route>Either</route>
		  <dccAspect>9</dccAspect>
	  </aspect>
	  <!-- Helper dummy masts -->

  </aspects>
